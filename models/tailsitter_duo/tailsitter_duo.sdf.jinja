{# ---------------------------------------------------------------- #}
{# macros #}
{# ---------------------------------------------------------------- #}
{%- macro inertial() -%}
<inertial>
  <mass>{{kwargs['m']}}</mass>
  <inertia>
    <ixx>{{kwargs['ixx']}}</ixx>
    <ixy>{{kwargs['ixy']}}</ixy>
    <ixz>{{kwargs['ixz']}}</ixz>
    <iyy>{{kwargs['iyy']}}</iyy>
    <iyz>{{kwargs['iyz']}}</iyz>
    <izz>{{kwargs['izz']}}</izz>
  </inertia>
</inertial>
{%- endmacro -%}
{%- macro channel(name) -%}
<channel name="{{name}}">
  <input_index>{{kwargs['i_index']}}</input_index>
  <input_offset>{{kwargs['i_offset']}}</input_offset>
  <input_scaling>{{kwargs['i_scaling']}}</input_scaling>
  <zero_position_disarmed>{{kwargs['zp_disarmed']}}</zero_position_disarmed>
  <zero_position_armed>{{kwargs['zp_armed']}}</zero_position_armed>
  <joint_control_type>{{kwargs['jc_type']}}</joint_control_type>
  <joint_name>{{name}}_joint</joint_name>
</channel>
{%- endmacro -%}
